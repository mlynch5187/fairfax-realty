<h1>Edit <%= @listing.address %></h1>
<center>
  <%= form_tag "/listings/#{@listing.id}", method: :patch do %>
    <%= label_tag :address %>
    <%= text_field_tag :address, "#{@listing.address}" %>

    <%= label_tag :city %>
    <%= text_field_tag :city, "#{@listing.city}" %>

    <%= label_tag :state %>
    <%= text_field_tag :state, "#{@listing.state}" %>

    <%= label_tag :zip %>
    <%= text_field_tag :zip,"#{@listing.zip}" %>

    <%= label_tag :description %>
    <%= text_field_tag :description,"#{@listing.zip}" %>

    <%= label_tag :image %>
    <%= text_field_tag :image,"#{@listing.zip}" %>

    <%= submit_tag 'Edit Listing' %>
  <% end %>
</center>
